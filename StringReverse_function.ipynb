   "source": [
    "# String Reverse using function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "e8d21cb6",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter any string: Hello DS26112021\n",
      "Reversed string is: 12021162SD olleH\n"
     ]
    }
   ],
   "source": [
    "def string_reverse(s,rev_s):\n",
    "      for ch in s[ : :-1]:\n",
    "            rev_s = rev_s + ch\n",
    "      return rev_s\n",
    "a = input(\"Enter any string: \")\n",
    "b = \"\"\n",
    "reverse = string_reverse(a,b)\n",
    "print(\"Reversed string is: \" + reverse)"
   ]
